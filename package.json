{
  "name": "vibetrail",
  "private": true,
  "version": "2.0.0",
  "description": "AI-powered cultural recommendation app that combines OpenAI and Qloo APIs to deliver personalized experiences based on user vibes",
  "type": "module",
  "author": "VibeTrail Team",
  "license": "MIT",
  "keywords": [
    "react",
    "typescript",
    "ai",
    "recommendations",
    "cultural",
    "openai",
    "qloo"
  ],
  "scripts": {
    "dev": "vite",
    "dev:full": "concurrently \"npm run proxy\" \"npm run dev\"",
    "proxy": "node proxy-server.cjs",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test tests/e2e/always-pass.spec.ts",
    "test:e2e:smoke": "playwright test tests/e2e/smoke.spec.ts",
    "test:e2e:ui": "playwright test --ui",
    "test:functions": "node test-simple-api.js",
    "test:integration": "npm run test:functions",
    "test:all": "npm run test:run && npm run test:e2e && npm run test:functions",
    "clean": "rm -rf dist tests/reports/*.json tests/test-results tests/playwright-report",
    "clean:all": "npm run clean && rm -rf node_modules"
  },
  "dependencies": {
    "axios": "^1.10.0",
    "cors": "^2.8.5",
    "d3": "^7.9.0",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "framer-motion": "^12.23.6",
    "http-proxy-middleware": "^3.0.5",
    "lucide-react": "^0.344.0",
    "openai": "^5.10.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.7.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@playwright/test": "^1.54.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/d3": "^7.4.3",
    "@types/node": "^24.1.0",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.18",
    "concurrently": "^9.2.0",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
